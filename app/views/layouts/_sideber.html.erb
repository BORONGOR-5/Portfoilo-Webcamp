<div class="col-xs-2 text-center", style="margin: 0 0 0 50px">
  <table class="table table-hover table-bordered">
    <thead bgcolor='#dcdcdc'>
      <th class="text-center">ジャンル検索</th>
    </thead>
    <tbody>
      <% genres.each do |genre| %>
        <tr>
          <td><%= link_to genres_sort_path(genre), data: {"turbolinks" => false} do %><%= genre.name %><% end %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>